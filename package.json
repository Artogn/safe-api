{
  "name": "safe-api",
  "version": "0.0.0",
  "description": "Front-end API to create applications on the SAFE Network created by MaidSafe",
  "keywords": [
    "safe",
    "safe network",
    "maidsafe",
    "api"
  ],
  "main": "lib/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "build:umd": "webpack src/index.js lib/index.js --config webpack.config.development.js",
    "develop": "webpack src/index.js lib/index.js --config webpack.config.development.js --progress --colors --watch"
  },
  "author": "Ephi Blanshey",
  "license": "MIT",
  "dependencies": {
    "firebase": "^2.4.0",
    "immutable": "^3.7.6",
    "react": "^0.14.7",
    "react-redux": "^4.1.1",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-as-promised": "^4.0.0",
    "webpack": "^1.12.11"
  }
}
